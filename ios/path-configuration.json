{
  "settings": {
    "enable-feature-x": true
  },
  "rules": [
    {
      "patterns": [
        "/new$",
        "/edit$",
        "/signin$",
        "/strada-form$"
      ],
      "properties": {
        "presentation": "modal"
      }
    },
    {
      "patterns": [
        "/numbers$"
      ],
      "properties": {
        "view-controller": "numbers"
      }
    },
    {
      "patterns": [
        "/numbers/[0-9]+$"
      ],
      "properties": {
        "view-controller": "numbersDetail",
        "presentation": "modal"
      }
    },
    {
      "patterns": [
        "/modal"
      ],
      "properties": {
        "presentation": "modal"
      }
    }
  ]
}
